<header class="site-header">
  <span class="site-header__logo-container">
    <a class="site-header__link" href="{{ '/' | url }}">
      <img
        class="site-header__logo"
        src="/static/img/logo.png"
        alt="Choose Nourish Logo"
        loading="lazy">
    </a>
    <a class="site-header__link" href="{{ '/' | url }}">
     <img
       class="site-header__minilogo"
       src="/static/img/logo.png"
       alt="Choose Nourish Logo"
       loading="lazy">
    </a>
  </span>
  <nav class="site-header__nav">
    {% set navPages = collections.all | eleventyNavigation %}
    <ul class="site-header__items">
    {%- for entry in navPages %}
      <li class="site-header__item">
        <a class="site-header__link" href="{{ entry.url | url }}"{% if entry.url == page.url %} data-current="current item"{% endif %}>{{ entry.title }}</a>
      </li>
    {%- endfor %}
    </ul>
  </nav>
</header>
